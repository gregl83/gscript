#!/usr/bin/env bash

DIR="$(dirname "$(readlink -f "$0")")"


# get gscript command
COMMAND=$1
COMMAND_SCRIPT=${DIR}/${COMMAND}


# verify command has script
if [ -e ${COMMAND_SCRIPT} ] && [ ! -d ${COMMAND_SCRIPT} ]
then
    # run auth
    . ${DIR}/auth

    . ${COMMAND_SCRIPT}
else
    echo "ERROR: unknown command $COMMAND"
    exit 1
fi